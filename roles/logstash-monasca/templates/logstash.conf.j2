input {
{% if logstash_input_kafka is defined %}
  kafka {
  {% for key, val in logstash_input_kafka.iteritems() %}
    {{ key }} => {{ val | to_nice_json }}
  {% endfor %}
  }
{% endif %}
{% if logstash_input_beats is defined %}
  beats {
  {% for key, val in logstash_input_beats.iteritems() %}
    {{ key }} => {{ val | to_nice_json }}
  {% endfor %}
  }
{% endif %}
}
output {
{% if logstash_output_elasticsearch is defined %}
  elasticsearch {
  {% for key, val in logstash_output_elasticsearch.iteritems() %}
    {{ key }} => {{ val | to_nice_json }}
  {% endfor %}
  }
{% endif %}
}
